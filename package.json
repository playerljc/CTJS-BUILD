{
  "name": "@ctsj/build",
  "version": "5.0.0",
  "description": "一个基于webpack的打包工具",
  "bin": {
    "ctbuild": "./bin/build.js",
    "ctcopy": "./bin/copy.js"
  },
  "scripts": {
    "devDll": "webpack --config webpack.dll.js",
    "startapp": "cross-env REAP_PATH=dev webpack-dev-server --open --config webpack.dev.js",
    "buildapp": "cross-env REAP_PATH=dev webpack --config webpack.prod.js",
    "buildpackage": "rimraf lib && babel src -d lib/src --ignore __tests__ && gulp"
  },
  "keywords": [
    "CTSJ-BUILD",
    "CTMobile-UI",
    "CTSJ-UI",
    "CTMobile",
    "playerljc",
    "webpack",
    "gulp",
    "babel"
  ],
  "author": "playerljc <playerljc@163.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/playerljc/CTSJ-BUILD.git"
  },
  "bugs": {
    "url": "https://github.com/playerljc/CTSJ-BUILD/issues"
  },
